{
  "name": "jira-metrics-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "lint:fix": "eslint '{src,test}/**/*.{ts,tsx}' --fix",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx,css,json,yaml}'",
    "format:check": "prettier --check '{src,test}/**/*.{ts,tsx,css,json}'",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format:check && npm run type-check && npm run type-check:tests && npm run test"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.9",
    "@thisbeyond/solid-dnd": "^0.7.5",
    "axios": "^1.8.1",
    "chart.js": "^4.4.8",
    "solid-js": "^1.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.0.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitest/coverage-v8": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "axios-mock-adapter": "^2.1.0",
    "canvas": "^3.1.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "solid-devtools": "^0.33.0",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^3.0.7"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.34.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "canvas",
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6"
}
